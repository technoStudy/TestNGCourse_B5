<suite name="Test Suite">

    <test name="Contact Us Test with only letter">
        <parameter name="enquiry" value="Your price is too high"></parameter>
        <classes>
            <class name="Day6.ContactUs._01_ContactUsFunction"></class>
        </classes>
    </test>

    <test name="Contact Us with only special Characters">
        <parameter name="enquiry" value="!@#$%!@@#$$$#@!```@@^*(*^%$"></parameter>
        <classes>
            <class name="Day6.ContactUs._01_ContactUsFunction"></class>
        </classes>
    </test>

    <test name="Contact Us with only numbers">
        <parameter name="enquiry" value="12345676543234567654323456765432345679009876"></parameter>
        <classes>
            <class name="Day6.ContactUs._01_ContactUsFunction"></class>
        </classes>
    </test>

    <test name="Contact Us with mixed characters">
        <parameter name="enquiry" value="qwedfghbvcde1234567654323!@#$%^%$#@#$cfdrtghDFGHF"></parameter>
        <classes>
            <class name="Day6.ContactUs._01_ContactUsFunction"></class>
        </classes>
    </test>

</suite>